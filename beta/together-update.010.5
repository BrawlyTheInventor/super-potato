
from random import *
from turtle import *
from config import *
if addons-enabled == True:
  if input("WARN: You have addons enabled. Do you want to use them? (type n for no)") == "n":
    print("Disableing addons...")
  else:
    print("Running addon scripts...")
    from addon import *
    if rundefaultprogram == False:
    exit()
player = Turtle()
player.penup()
def forward():
	player.forward(customforward)
def backward():
	player.backward(custombackward)
def right():
	player.right(customright)
def left():
	player.left(customleft)
type = choice(["potato", "tomato", "chicken", "SUPER POTATO"])
print("It's a " + type)
if type == "tomato":
	# registering the image 
	# as a new shape 
	register_shape('tomato.jpeg') 
  
# setting the image as cursor 
	shape('tomato.jpeg') 
elif type == "potato":
	# registering the image 
# as a new shape 
	register_shape('potato.jpeg') 
  
# setting the image as cursor 
	shape('potato.jpeg') 
elif type == "chicken":
	# registering the image 
# as a new shape 
	register_shape('chicken.jpeg')
	shape('chicken.jpeg')
elif type == "SUPER POTATO":
	# registering the image 
# as a new shape 
	register_shape('superpotato.jpeg') 
  
# setting the image as cursor 
	shape('superpotato.jpeg')
penup()
goto(randint(1, 10000000), randint(1, 10000000))
screen = Screen()
screen.setup(800, 560)
screen.setworldcoordinates(0, 0, 800, 560)
screen.listen()
screen.onkey(forward, 'w')
screen.onkey(backward, 's')
screen.onkey(right, 'a')
screen.onkey(left, 'd')
while True:
	penup()
	goto(randint(1, 100), randint(1, 100))
